    {
      "name": "2. Driver Management",
      "item": [
        {
          "name": "Update Driver Status",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{driverToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"status\": \"ONLINE\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/drivers/status",
              "host": ["{{baseUrl}}"],
              "path": ["drivers", "status"]
            }
          },
          "response": []
        },
        {
          "name": "Update Driver Location",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{driverToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"lat\": 40.7128,\n  \"lng\": -74.0060\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/drivers/location",
              "host": ["{{baseUrl}}"],
              "path": ["drivers", "location"]
            }
          },
          "response": []
        },
        {
          "name": "Find Nearby Drivers",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/drivers/nearby?lat=40.7128&lng=-74.0060&radius=5",
              "host": ["{{baseUrl}}"],
              "path": ["drivers", "nearby"],
              "query": [
                {
                  "key": "lat",
                  "value": "40.7128"
                },
                {
                  "key": "lng",
                  "value": "-74.0060"
                },
                {
                  "key": "radius",
                  "value": "5"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Driver Statistics",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{driverToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/drivers/stats",
              "host": ["{{baseUrl}}"],
              "path": ["drivers", "stats"]
            }
          },
          "response": []
        },
        {
          "name": "Estimate Ride",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/drivers/estimate?pickupLat=40.7128&pickupLng=-74.0060&dropoffLat=40.7589&dropoffLng=-73.9851",
              "host": ["{{baseUrl}}"],
              "path": ["drivers", "estimate"],
              "query": [
                {
                  "key": "pickupLat",
                  "value": "40.7128"
                },
                {
                  "key": "pickupLng",
                  "value": "-74.0060"
                },
                {
                  "key": "dropoffLat",
                  "value": "40.7589"
                },
                {
                  "key": "dropoffLng",
                  "value": "-73.9851"
                }
              ]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "3. Ride Management",
      "item": [
        {
          "name": "Create Ride Request",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200 || pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    if (response.id) {",
                  "        pm.collectionVariables.set('rideId', response.id);",
                  "        console.log('Ride ID saved:', response.id);",
                  "    }",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"pickupLat\": 40.7128,\n  \"pickupLng\": -74.0060,\n  \"dropoffLat\": 40.7589,\n  \"dropoffLng\": -73.9851\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/rides",
              "host": ["{{baseUrl}}"],
              "path": ["rides"]
            }
          },
          "response": []
        },
        {
          "name": "Get Ride History",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/rides/history?page=1&limit=10",
              "host": ["{{baseUrl}}"],
              "path": ["rides", "history"],
              "query": [
                {
                  "key": "page",
                  "value": "1"
                },
                {
                  "key": "limit",
                  "value": "10"
                }
              ]
            }
          },
          "response": []
        },
        {
          "name": "Get Active Rides",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/rides/active",
              "host": ["{{baseUrl}}"],
              "path": ["rides", "active"]
            }
          },
          "response": []
        },
        {
          "name": "Get Specific Ride",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/rides/{{rideId}}",
              "host": ["{{baseUrl}}"],
              "path": ["rides", "{{rideId}}"]
            }
          },
          "response": []
        },
        {
          "name": "Accept Ride (Driver)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{driverToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/rides/{{rideId}}/accept",
              "host": ["{{baseUrl}}"],
              "path": ["rides", "{{rideId}}", "accept"]
            }
          },
          "response": []
        },
        {
          "name": "Start Ride",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{driverToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/rides/{{rideId}}/start",
              "host": ["{{baseUrl}}"],
              "path": ["rides", "{{rideId}}", "start"]
            }
          },
          "response": []
        },
        {
          "name": "Complete Ride",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              },
              {
                "key": "Authorization",
                "value": "Bearer {{driverToken}}"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"actualDistance\": 2.5,\n  \"actualTimeMinutes\": 8\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/rides/{{rideId}}/complete",
              "host": ["{{baseUrl}}"],
              "path": ["rides", "{{rideId}}", "complete"]
            }
          },
          "response": []
        },
        {
          "name": "Cancel Ride",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{adminToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/rides/{{rideId}}/cancel",
              "host": ["{{baseUrl}}"],
              "path": ["rides", "{{rideId}}", "cancel"]
            }
          },
          "response": []
        }
      ]
